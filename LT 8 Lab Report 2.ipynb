    "def hybrid_search(query, vectorizer, nmf, W, tfidf_matrix, alpha=0.5, top_k=20):\n",
    "    # Query in tfidf space\n",
    "    q_tfidf = vectorizer.transform([query])\n",
    "    sim_tfidf = cosine_similarity(q_tfidf, tfidf_matrix).ravel()\n",
    "\n",
    "    # Query in nmf topic space\n",
    "    q_topic = nmf.transform(q_tfidf)\n",
    "    sim_nmf = cosine_similarity(q_topic, W).ravel()\n",
    "\n",
    "    # Combine\n",
    "    hybrid = alpha * sim_tfidf + (1 - alpha) * sim_nmf\n",
    "\n",
    "    idx = hybrid.argsort()[::-1][:top_k]\n",
    "    return idx, hybrid[idx]"
   ]
